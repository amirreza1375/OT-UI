<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android">    <item android:state_pressed="false">        <shape>            <stroke                android:color="@color/main_calendar_btn_center"                android:width="3dp"/>            <corners android:radius="8dp"/>        </shape>    </item>    <item android:state_pressed="true">        <shape>            <stroke                android:color="@color/main_calendar_btn_center_p"                android:width="3dp"/>            <corners android:radius="8dp"/>        </shape>    </item></selector>