<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android"><item android:state_pressed="false">    <shape>        <gradient android:startColor="@color/main_calendar_btn_start"            android:centerColor="@color/main_calendar_btn_center"            android:endColor="@color/main_calendar_btn_end"            android:angle="180"/>        <corners android:radius="8dp"/>    </shape></item>    <item android:state_pressed="true">        <shape>            <gradient android:startColor="@color/main_calendar_btn_start_p"                android:centerColor="@color/main_calendar_btn_center_p"                android:endColor="@color/main_calendar_btn_end_p"                android:angle="180"/>            <corners android:radius="8dp"/>        </shape>    </item></selector>